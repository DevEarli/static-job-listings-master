[1m--- /dev/null[m
[1m+++ b/script.js[m
[36m@@ -0,0 +1,64 @@[m
[32m+[m[32mconst buttons = document.querySelectorAll('.button');[m
[32m+[m[32mconst searchContainer = document.querySelector('.search-container');[m
[32m+[m[32mconst clear = searchContainer.lastElementChild;[m
[32m+[m[32mconst cards = document.querySelectorAll('.card');[m
[32m+[m[32mlet state = [];[m
[32m+[m
[32m+[m[32mbuttons.forEach(button => {[m
[32m+[m[32m    button.addEventListener('click',function(){[m
[32m+[m[32m        if(!state.includes(button.innerHTML)){[m
[32m+[m[32m            state.push(button.innerHTML);[m
[32m+[m[32m            SearchBox();[m
[32m+[m[32m            Filter();[m
[32m+[m[32m            console.log(state);[m
[32m+[m[32m        }[m
[32m+[m[32m    });[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32mclear.addEventListener('click',function(){[m
[32m+[m[32m    state = [];[m
[32m+[m[32m    SearchBox();[m
[32m+[m[32m    Filter();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mfunction SearchBox(){[m
[32m+[m[32m    const Box = searchContainer.style;[m
[32m+[m[32m    state.length >0 ? Box.display = 'flex': Box.display = 'none';[m
[32m+[m[32m    let el = '';[m
[32m+[m[32m    state.map(a => {[m
[32m+[m[32m        el+=`<div class="search-button">[m
[32m+[m[32m            <p>${a}</p>[m
[32m+[m[32m             <div class="remove"></div>[m
[32m+[m[32m        </div>`;[m
[32m+[m[32m    });[m
[32m+[m[41m    [m
[32m+[m[32m    searchContainer.firstElementChild.innerHTML = el;[m
[32m+[m[32m    const remove = document.querySelectorAll('.remove');[m
[32m+[m[32m    remove.forEach(el => {[m
[32m+[m[32m        el.addEventListener('click',function(){[m
[32m+[m[32m            let value = this.previousElementSibling.innerHTML;[m
[32m+[m[32m            state = state.filter(a => a!=value);[m
[32m+[m[32m            SearchBox();[m
[32m+[m[32m            Filter();[m
[32m+[m[32m        });[m
[32m+[m[32m    })[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction Filter(){[m
[32m+[m[32m    console.log(cards);[m
[32m+[m
[32m+[m[32m    cards.forEach(card => {[m
[32m+[m[32m        card.style.display = ' flex';[m
[32m+[m[32m        const Buttons = Array.from(card.querySelectorAll('.button'));[m
[32m+[m[32m        const Texts = Buttons.map(Button => Button.innerHTML);[m
[32m+[m
[32m+[m
[32m+[m[32m        for(let i = 0;i<state.length;i++){[m
[32m+[m[32m            if(Texts.includes(state[i])){[m
[32m+[m[32m                continue;[m
[32m+[m[32m            }else{[m
[32m+[m[32m                card.style.display = 'none';[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    })[m
[32m+[m[32m}[m
